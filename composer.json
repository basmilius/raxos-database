{
	"name": "raxos/database",
	"description": "Database ORM and Query Builder.",
	"license": "mit",
    "minimum-stability": "dev",
    "prefer-stable" : true,
	"type": "library",
	"authors": [
		{
			"name": "Bas Milius",
			"email": "bas@mili.us"
		}
	],
	"autoload": {
        "files": [
            "src/Raxos/Database/Query/functions.php"
        ],
		"psr-4": {
			"Raxos\\Database\\": "src/Raxos/Database"
		}
	},
	"config": {
		"platform": {
			"php": "8.3"
		}
	},
    "repositories": [
        {
            "type": "path",
            "url": "../*",
            "symlink": true
        }
    ],
	"require": {
        "ext-pdo" : "*",
		"jetbrains/phpstorm-attributes": "^1.1",
        "raxos/foundation": "*"
	}
}
